        $('#es-form-mobile').submit(function (e) {
            var base = "/search";
            var searchTerm = $(this).find('.es-form-field-query').val();
            var searchQuery = base + "?q=" + searchTerm;

            e.preventDefault();
            window.location.href = searchQuery;
        });
