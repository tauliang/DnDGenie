    // hide the sign out button for anonymous users
    document.addEventListener("DOMContentLoaded", function () {
        try {
            if (!Cobalt.User.IsAuthenticated) {
                $('.responsive-sign-out').addClass('hide');
            }
        }
        catch (err) {
            console.log(err.message);
        }
    });
</script>
